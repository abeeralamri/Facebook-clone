<p style="color: green"><%= notice %></p>

<h1>Profiles</h1>

<div id="profiles">
  <% @profiles.each do |profile| %>
    <%= render profile %>
    <p>
      <%= link_to t("Show this profile"), profile ,class:"btn btn-primary" %>
    </p>
  <% end %>
</div>

<%= link_to t("new"), new_profile_path ,class:"btn btn-primary" %>
